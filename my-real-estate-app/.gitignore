# =========================================
# React App .gitignore
# =========================================

# dependencies
/node_modules/

# build output (uncomment build/ if your host builds the app automatically)
/build/

# local env files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# editor directories and files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# OS-specific files
.DS_Store
Thumbs.db

# optional: cache and misc
.cache/
dist/
coverage/
*.tgz

